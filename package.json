{
  "name": "@nttb/tt-match-ranking",
  "version": "1.0.0",
  "description": "Table tennis match ranking library",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "tsc",
    "build:doc": "typedoc --out docs src/index.ts",
    "prepack": "npm run-script build; npm run-script build:doc",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src",
    "format": "prettier --check src",
    "format:write": "prettier --write src",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:watch:coverage": "jest --watchAll --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NTTB/tt-match-ranking.git"
  },
  "keywords": [
    "table tennis",
    "ranking",
    "utility"
  ],
  "author": "Wouter Lindenhof",
  "license": "EUPL",
  "bugs": {
    "url": "https://github.com/NTTB/tt-match-ranking/issues"
  },
  "homepage": "https://github.com/NTTB/tt-match-ranking#readme",
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "eslint": "^7.20.0",
    "eslint-plugin-import": "^2.22.1",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.1",
    "typedoc": "^0.20.30",
    "typescript": "^4.1.5"
  }
}
